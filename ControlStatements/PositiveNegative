import java.util.Scanner;

public class Main{

    public static void main(String[] args) {

        Scanner reader = new Scanner(System.in);
        int a = reader.nextInt();
        if(a>0)
        {
            System.out.println("positive");
        }
        else if (a<0)
        {
            System.out.println("Negative");
        }
        else
        {
            System.out.println("Zero");
        }
        }
        
}
