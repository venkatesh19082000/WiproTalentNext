import java.util.Scanner;
public class Main{
   public static void main(String args[])
   {
     Scanner s=new Scanner(System.in);

     char ch=s.next().charAt(0);
     char b=s.next().charAt(0);
     if(ch<b)
     {
         System.out.println(ch+","+b);
     }
     else{
         System.out.println(b+","+ch);
     }
   }
}
